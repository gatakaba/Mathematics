    "ar1  = 2*np.cos(2*np.pi*0.9);\n",
    "\n",
    "def update(x,x_prev):\n",
    "    k=ar1+np.random.normal()*0.2\n",
    "    x_next = ar1 * x -x_prev\n",
    "    return  x_next,x\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 185,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "\n",
    "x=0\n",
    "x_prev=0.1\n",
    "l=[]\n",
    "for i in range(400):\n",
    "\n",
    "    x,x_prev=update(x,x_prev)\n",
    "    l.append([x,x_prev])\n",
    "    \n"
   ]
